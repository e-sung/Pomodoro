  pod 'SwiftDate'
  # Pods for Pomodoro

  target 'PomodoroTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PomodoroUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
